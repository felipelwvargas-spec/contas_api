import 'package:dio/dio.dart';
import 'package:amigo_secreto/services/account_services.dart';

final dio = Dio();

void request() async { 
  Response response;// Declara a variável response do tipo Response
  response = await dio.get("https://api.github.com/gists/b49d630bd597c03ed8b9b90d498e7f5a"); // Faz a requisição GET para a URL especificada
  print(response.data.toString()); 

  response = await dio.get(
    '/test',
    queryParameters: {'id': 12, 'name': 'dio'},
  );
  print(response.data.toString());
}